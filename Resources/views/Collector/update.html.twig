{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="22" height="28" alt="Update" src="data:image/png;base64," />
        <span class="sf-toolbar-status{% if collector.unstablePackageCount %} sf-toolbar-status-red{% endif %}">{{ collector.unstablePackageCount }}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Packages</b>
            <span>{{ collector.packageCount }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Unstable packages</b>
            <span class="sf-toolbar-status sf-toolbar-status-{{ collector.unstablePackageCount > 0 ? 'red' : 'green' }}">{{ collector.unstablePackageCount }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon">
      <img width="39" height="30" alt="Update" src="data:image/png;base64," />
    </span>
    <strong>UPDATE</strong>
</span>
{% endblock %}

{% block panel %}
    <h2>Packages</h2>

    <table class="routing">
    <thead><tr><th>Name</th><th>Required</th><th>Version</th><th>Description</th></tr></thead>
    <tbody>
    {% for i, packages in collector.packages %}
        <tr class="{{ packages.unstable ? 'almost' }}">
            <td>{{ packages.name }}</td>
            <td>{{ packages.required }}</td>
            <td>{{ packages.version }}</td>
            <td>{{ packages.description }}</td>
        </tr>
    {% endfor %}
    </tr></tbody></table>

    <h2>Update</h2>

    <form method="post" action="{{ url('lsw_automatic_update.update.dry_run') }}" style="font-size: 14px;">
        <label for="secret">Secret</label>
        <input name="secret" type="password" style="width:400px;"/><br/><br/>
        <input type="submit" value="Step 1 of 2: Dry-Run"/>
    </form>

{% endblock %}